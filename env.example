# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_JWT_SECRET=your_jwt_secret

# Database Configuration (Postgres)
POSTGRES_URL=your_postgres_connection_string
POSTGRES_PRISMA_URL=your_postgres_prisma_url
POSTGRES_URL_NON_POOLING=your_postgres_non_pooling_url
POSTGRES_USER=your_postgres_user
POSTGRES_HOST=your_postgres_host
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=your_postgres_database

# Google Maps (Server-side only)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google Calendar API (Required for appointment scheduling)
GOOGLE_CALENDAR_CLIENT_ID=your_google_calendar_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_calendar_client_secret
GOOGLE_CALENDAR_REDIRECT_URI=https://your-domain.vercel.app/api/auth/google-calendar/callback
GOOGLE_CALENDAR_REFRESH_TOKEN=your_google_calendar_refresh_token

# Google Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS=your_ga_tracking_id

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_LOCAL_CALLBACK_URL=http://localhost:3000/auth/callback

# SheetDB Configuration
NEXT_PUBLIC_SHEETDB_API_URL=your_sheetdb_api_url
SHEETDB_API_KEY=your_sheetdb_api_key

# Feature Flags (Environment Variables)
# Set to "true" to enable, "false" or omit to disable
NEXT_PUBLIC_FEATURE_WAITING_LIST=false
NEXT_PUBLIC_FEATURE_FEEDBACK=true
NEXT_PUBLIC_FEATURE_MAINTENANCE_MODE=false
NEXT_PUBLIC_FEATURE_NEW_USER_REGISTRATION=true
NEXT_PUBLIC_FEATURE_MENTOR_VERIFICATION=true
