# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# OAuth Callback URLs (Public - used for redirects)
NEXT_PUBLIC_LOCAL_CALLBACK_URL=http://localhost:3000/auth/callback

# Site URL (for redirects)
NEXT_PUBLIC_SITE_URL=https://your-app-domain.vercel.app # http://localhost:3000 

# Google Configuration (Server-side only)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# LinkedIn Configuration (Server-side only)
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret

# GitHub Configuration (Server-side only)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Google Maps API (Server-side only)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# SheetDB Configuration
SHEETDB_API_KEY=your-sheetdb-api-key
NEXT_PUBLIC_SHEETDB_API_URL=https://sheetdb.io/api/v1/your-sheet-id

# OAuth Providers for Supabase (Server-side only)
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=your-google-client-secret
SUPABASE_AUTH_EXTERNAL_LINKEDIN_SECRET=your-linkedin-client-secret
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=your-github-client-secret
