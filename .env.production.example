# Supabase Configuration for Production
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# OAuth Callback URLs
NEXT_PUBLIC_OAUTH_CALLBACK_URL=https://your-project.supabase.co/auth/v1/callback
NEXT_PUBLIC_LOCAL_CALLBACK_URL=https://your-domain.com/auth/callback

# Site URL (for redirects)
NEXT_PUBLIC_SITE_URL=https://your-domain.com/

# OAuth Provider Credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret

GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

NEXT_PUBLIC_RESET_PASSWORD_URL=https://your-domain.com/reset-password

NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Production specific
NEXT_PUBLIC_FEATURE_WAITING_LIST=false
NEXT_PUBLIC_FEATURE_NEW_USER_REGISTRATION=true